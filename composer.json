{
  "name": "prob/framework",
  "description": "A simple PHP framework",
  "homepage": "https://github.com/jongpak/prob-framework",
  "keywords": ["framework"],
  "type": "project",
  "license": "MIT",

  "minimum-stability": "dev",
  "prefer-stable": true,

  "require": {
    "twig/twig": "*",
    "doctrine/orm": "*",
    "jbzoo/event": "*",
    "psr/http-message": "*",
    "prob/router": "dev-master",
    "jongpak/array-key-glue": "dev-master",
    "zendframework/zend-diactoros": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Controller\\": "app/",
      "Core\\": "core/"
    }
  }
}
